{
  "name": "Game-of-credit",
  "version": "0.3.2",
  "description": "3D first person shooter in web browser",
  "author": "Jan Svager <jan@svager.cz> (http://svager.cz)",
  "contributors": [
    "Michael Muller"
  ],
  "license": "Apache License 2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:svagi/Game-of-credit.git"
  },
  "bugs": {
    "url": "https://github.com/svagi/Game-of-credit/issues"
  },
  "keywords": [
    "WebGL",
    "Three.js",
    "3D",
    "game",
    "credit"
  ],
  "bin": "./bin/www",
  "config": {
    "port": "8000"
  },
  "main": "./app.coffee",
  "os": [
    "win32",
    "darwin"
  ],
  "scripts": {
    "build": "webpack",
    "production": "webpack -p && NODE_ENV=production coffee ./bin/www",
    "dev": "webpack-dev-server --progress --colors --inline -d",
    "start": "npm run dev & coffee ./bin/www",
    "test": "mocha"
  },
  "dependencies": {
    "coffee-script": "^1.9.0",
    "express": "^4.11.0",
    "jade": "^1.9.2",
    "keymaster": "^1.6.2",
    "socket.io": "^1.3.4",
    "stats-js": "^1.0.0-alpha1"
  },
  "devDependencies": {
    "coffee-loader": "^0.7.2",
    "css-loader": "^0.9.1",
    "json-loader": "^0.5.1",
    "mocha": "^2.1.0",
    "style-loader": "^0.8.3",
    "supertest": "^0.15.0",
    "webpack": "^1.5.3",
    "webpack-dev-server": "^1.7.0"
  }
}
